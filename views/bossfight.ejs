<!DOCTYPE html>
<html lang="nl">
  <%- include('./partials/head') -%>
  <body class="coop">
    <%- include('./partials/header') -%>
    <main>
      <div>
        <ul id="online">
          <li>People online:</li>
        </ul>
      </div>

      <section class="usernameSection">
        <form name-form>
          <label>What's your name?</label>
          <input type="text" name="username" value="Tim" placeholder="You name" name_input>
          <button><span>Start searching</span></button>
        </form>
      </section>

        <section>
          <% const img = data.webImage.url.slice(0,-3)+"=s1000" %>
          <section class="bossfight" id="artDetail">
            <section class="art">
              <img src="<%= img %>" alt="<%= data.title %>" paintItem id="<%= data.id %>" width="100vmin" height="auto"/>
              <h2><%= data.title %> </h2>
            </section>
          </section>
        </section>

        <section id="humanSpawner"></section>

        <img class="rijksBackground" src="/images/background.png">

        <form speakForm>
          <input type="text" speakForm-input autocomplete="off" placeholder="Type to chat..." class="chatStyling"/>
        </form>
    </main>
    <%- include('./partials/footer') -%>
  </body>
</html>
